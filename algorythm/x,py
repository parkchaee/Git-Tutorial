guguLine=" "
for i in range(2,10,1):
    print("#%d단#" %i, end=" ")
    for k in range(1,10,1):
        guguLine=guguLine+str("%dx%d=%d "%(i,k,i*k))
print(guguLine)


guguLine=" "
for i in range(2,10):
    guguLine+=("#%d단#"%i)
print(guguLine)
for i in range(1,10):
    guguLine=" "
    for k in range(2,10):
         guguLine+=str("%2dx%2d=%2d"%(k,i,k*i))
    print(guguLine)